{
    "compilerOptions": {
        "module": "Node16",
        "moduleResolution": "Node16",
        "target": "ES2022",
        "lib": [
            "ES2024",
            "webworker"
        ],
        "rootDir": "src",
        "strict": true, /* enable all strict type-checking options */
        "outDir": "./dist",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "declaration": true,
        "stripInternal": true,
        "sourceMap": true,
        "declarationMap": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false
    }
}